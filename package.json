{
  "name": "regexp-ts",
  "version": "0.0.1",
  "description": "A TypeScript library that provides a programmatic and human readable way to create strongly-typed regular expressions",
  "author": "P. Christopher Bowers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pcbowers/regexp-ts.git"
  },
  "bugs": {
    "url": "https://github.com/pcbowers/regexp-ts/issues"
  },
  "keywords": [
    "regular",
    "expressions",
    "javascript",
    "typescript",
    "typed"
  ],
  "type": "module",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vitest",
    "test": "vitest",
    "build": "tsup src/index.ts --format cjs,esm,iife --dts --minify --sourcemap",
    "preview": "vite preview",
    "coverage": "vitest run --coverage",
    "lint": "prettier --check . && eslint --ignore-path .gitignore . && tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitest/coverage-c8": "^0.28.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "2.8.3",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.1"
  },
  "dependencies": {
    "zod": "^3.20.6"
  }
}
