{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": ["dist/**"]
    },
    "test": {},
    "check-exports": {
      "dependsOn": ["build"]
    },
    "lint": {},
    "publish-packages": {
      "dependsOn": ["build", "check-exports", "lint"],
      "cache": false
    }
  }
}
